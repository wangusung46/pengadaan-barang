	<queryString>
		<![CDATA[select
	a.id,
	a.barang,
	a.satuan,
	a.volume,
	d.harga,
	b.tanggal_pembelian 
from
	detail_pembelian a
left join pembelian b on
	a.id_pembelian = b.id
left join kontrak c on
	b.id_kontrak = c.id
left join detail_kontrak d on
	c.id = d.id_kontrak
where b.tanggal_pembelian between $P{dari} and $P{sampai}]]>
	</queryString>
